---
const { page } = Astro.props;
const prevUrl = page.url.prev;
const nextUrl = page.url.next;
const currentUrl = page.url.current;
console.log(page);
---

<div class="text-riso-blue my-6 flex">
  {
    prevUrl && (
      <a class="hover:underline" href={prevUrl}>
        {"<< Previous Page"}
      </a>
    )
  }

  {
    nextUrl && (
      <a class="ml-auto hover:underline" href={nextUrl}>
        {"Next Page >>"}
      </a>
    )
  }
</div>
