---
const { page } = Astro.props;
const prevUrl = page.url.prev;
const nextUrl = page.url.next;
const currentPage = page.currentPage;
---

<div class="mb-10 mx-auto flex justify-between">
  {prevUrl && <a href={prevUrl}>Previous Page</a>}
  <a href="/blog"> Home</a>
  {nextUrl && <a href={nextUrl}>Next Page</a>}
</div>
